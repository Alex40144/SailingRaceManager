generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Boats {
  id   String @id @unique @default(uuid())
  Name String @unique
  Crew Int
  PY   Int
}

model Clubs {
  id     String @id @unique @default(uuid())
  Name   String @unique
}

model Series {
  id      String  @id @unique @default(uuid())
  Name    String  @unique
  clubsId String?
}

model Race {
  id       String   @id @unique @default(uuid())
  Number   Int
  DateTime DateTime
  OOD      String
  AOD      String
  SO       String
  ASO      String
  Results  Json?
  seriesId String?
}
